{
  "name": "nestjs-boilerplate",
  "version": "1.2.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "typeorm": "env-cmd ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:generate",
    "postmigration:generate": "npm run lint -- --fix",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:run",
    "migration:revert": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:revert",
    "schema:drop": "npm run typeorm -- --dataSource=src/database/data-source.ts schema:drop",
    "seed:create:relational": "hygen seeds create-relational",
    "generate:resource:relational": "hygen generate relational-resource",
    "postgenerate:resource:relational": "npm run lint -- --fix",
    "add:property:to-relational": "hygen property add-to-relational",
    "postadd:property:to-relational": "npm run lint -- --fix",
    "seed:run:relational": "ts-node -r tsconfig-paths/register ./src/database/seeds/relational/run-seed.ts",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:swc": "nest start -b swc -w",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json",
    "test:e2e:relational:docker": "docker compose -f docker-compose.relational.test.yaml --env-file env-example-relational -p tests up -d --build && docker compose -f docker-compose.relational.test.yaml -p tests exec api /opt/wait-for-it.sh -t 0 localhost:3000 -- npm run test:e2e -- --watchAll --runInBand && docker compose -f docker-compose.relational.test.yaml -p tests down && docker compose -p tests rm -svf",
    "prepare": "is-ci || husky",
    "release": "release-it",
    "generate:secret-key": "ts-node -r tsconfig-paths/register ./src/utils/scripts/generator.script.ts secret",
    "generate:password": "ts-node -r tsconfig-paths/register ./src/utils/scripts/generator.script.ts password",
    "docker:dev:up": "docker compose -f docker-compose.yaml up -d maildev postgres redis",
    "docker:app:up": "docker compose -f docker-compose.yaml up -d app",
    "docker:dev:down": "docker compose -f docker-compose.yaml down -v maildev postgres redis",
    "docker:app:down": "docker compose -f docker-compose.yaml down -v app",
    "git:branch:list": "git branch -a",
    "git:branch:update": "git remote update origin --prune",
    "git:branch:checkout": "git checkout",
    "git:fetch:all": "git fetch --all --prune",
    "git:pull": "git pull",
    "git:pull:all": "git pull --all",
    "git:status": "git status",
    "git:log": "git log --oneline --graph --all",
    "git:rebase:main": "git fetch origin main && git rebase origin/main",
    "git:rebase:continue": "git rebase --continue",
    "commands": "npx ntl",
    "app:kill": "sudo kill -9 $(sudo lsof -t -i:3000)"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.758.0",
    "@aws-sdk/s3-request-presigner": "3.758.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "11.0.20",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.1.2",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/microservices": "^11.1.2",
    "@nestjs/mongoose": "11.0.3",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "11.1.2",
    "@nestjs/platform-socket.io": "^11.1.3",
    "@nestjs/swagger": "11.1.4",
    "@nestjs/typeorm": "11.0.0",
    "@nestjs/websockets": "^11.1.3",
    "@scalar/nestjs-api-reference": "^0.4.11",
    "@types/multer-s3": "3.0.3",
    "@types/prompts": "2.4.9",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.8",
    "apple-signin-auth": "1.7.9",
    "axios": "^1.9.0",
    "bcryptjs": "3.0.2",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "date-fns": "^4.1.0",
    "dotenv": "16.5.0",
    "google-auth-library": "9.15.1",
    "handlebars": "4.7.8",
    "ioredis": "^5.6.1",
    "jwks-rsa": "^3.2.0",
    "minio": "^8.0.5",
    "mongoose": "8.13.2",
    "mongoose-autopopulate": "^1.1.0",
    "ms": "2.1.3",
    "multer": "1.4.5-lts.2",
    "multer-s3": "3.0.1",
    "nestjs-i18n": "10.5.1",
    "nodemailer": "6.10.1",
    "passport": "0.7.0",
    "passport-anonymous": "1.0.1",
    "passport-jwt": "4.0.1",
    "pg": "8.14.1",
    "redis": "^4.7.1",
    "reflect-metadata": "0.2.2",
    "rimraf": "6.0.1",
    "rxjs": "7.8.2",
    "socket.io": "^4.8.1",
    "source-map-support": "0.5.21",
    "swagger-themes": "^1.4.3",
    "swagger-ui-express": "5.0.1",
    "typeorm": "0.3.24",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@nestjs/cli": "11.0.7",
    "@nestjs/schematics": "11.0.5",
    "@nestjs/testing": "11.1.2",
    "@release-it/conventional-changelog": "10.0.1",
    "@swc/cli": "0.6.0",
    "@swc/core": "1.11.21",
    "@types/express": "5.0.1",
    "@types/jest": "29.5.14",
    "@types/minio": "^7.1.0",
    "@types/ms": "2.1.0",
    "@types/multer": "1.4.12",
    "@types/node": "22.15.21",
    "@types/passport-anonymous": "1.0.5",
    "@types/passport-jwt": "4.0.1",
    "@types/supertest": "6.0.3",
    "@typescript-eslint/eslint-plugin": "8.29.1",
    "@typescript-eslint/parser": "8.29.1",
    "env-cmd": "10.1.0",
    "eslint": "9.27.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.2.6",
    "globals": "^16.0.0",
    "husky": "9.1.7",
    "hygen": "6.2.11",
    "is-ci": "4.1.0",
    "jest": "29.7.0",
    "ntl": "^5.1.0",
    "prettier": "3.5.3",
    "prompts": "2.4.2",
    "release-it": "19.0.2",
    "socket.io-client": "^4.8.1",
    "supertest": "7.1.1",
    "ts-jest": "29.3.4",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.8.1",
    "typescript": "5.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "infile": "CHANGELOG.md",
        "preset": {
          "name": "conventionalcommits",
          "types": [
            {
              "type": "chore(deps)",
              "section": "Dependency Upgrades"
            },
            {
              "type": "fix(deps)",
              "section": "Dependency Upgrades"
            },
            {
              "type": "feat",
              "section": "Features"
            },
            {
              "type": "fix",
              "section": "Bug Fixes"
            },
            {
              "type": "perf",
              "section": "Performance Improvements"
            },
            {
              "type": "revert",
              "section": "Reverts"
            },
            {
              "type": "docs",
              "section": "Documentation"
            },
            {
              "type": "refactor",
              "section": "Code Refactoring"
            },
            {
              "type": "test",
              "section": "Tests"
            },
            {
              "type": "ci",
              "section": "Continuous Integration"
            }
          ]
        }
      }
    }
  }
}
