{
  "name": "nestjs-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "is-ci || husky install",
    "typeorm": "env-cmd ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "seeds": "env-cmd ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate -n",
    "migration:create": "npm run typeorm -- migration:create -n",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "schema:drop": "npm run typeorm -- schema:drop",
    "seed:config": "npm run seeds -- config",
    "seed:run": "npm run seeds -- seed",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.5.1",
    "@nestjs/common": "^7.6.13",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.13",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.13",
    "@nestjs/swagger": "^4.7.15",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjsx/crud": "^4.6.2",
    "@nestjsx/crud-typeorm": "^4.6.2",
    "apple-signin-auth": "^1.4.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.13.1",
    "fb": "^2.0.0",
    "google-auth-library": "^7.0.2",
    "handlebars": "^4.7.7",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nestjs-i18n": "^8.0.11",
    "nodemailer": "^6.5.0",
    "passport": "^0.4.1",
    "passport-anonymous": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "swagger-ui-express": "^4.1.6",
    "twitter": "^1.7.1",
    "typeorm": "^0.2.31",
    "typeorm-seeding": "^1.6.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.6",
    "@nestjs/schematics": "^7.2.8",
    "@nestjs/testing": "^7.6.13",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.11",
    "@types/facebook-js-sdk": "^3.3.1",
    "@types/jest": "26.0.20",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.31",
    "@types/passport-anonymous": "^1.0.3",
    "@types/passport-jwt": "^3.0.5",
    "@types/supertest": "^2.0.8",
    "@types/twitter": "^1.7.0",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "aws-sdk": "^2.857.0",
    "env-cmd": "^10.1.0",
    "eslint": "7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.3",
    "is-ci": "^3.0.0",
    "jest": "26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "26.5.3",
    "ts-loader": "^8.0.17",
    "ts-node": "9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": "14.x"
  }
}
